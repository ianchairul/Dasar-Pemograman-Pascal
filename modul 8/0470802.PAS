PROGRAM isilarik;
USES CRT;
TYPE
larik = array [1..10] of integer;
VAR nilai : larik;
    i : integer;

BEGIN
CLRSCR;
nilai[1]:=5;
nilai[2]:=4-nilai[1];
nilai[3]:=0;
nilai[4]:=nilai[2];
nilai[5]:=4-nilai[2];
nilai[6]:=nilai[nilai[1]];
nilai[7]:=nilai[nilai[1]-2];
nilai[8]:=nilai[2];
nilai[9]:=nilai[3+4];
nilai[10]:=9;
FOR i := 1 to 10 do
WRITELN('nilai index ke- ',i,'=',nilai[i]:5);
writeln('');
READLN;
END.