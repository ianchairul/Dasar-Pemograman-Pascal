program perkalian;
uses crt;
var i,j,hasil : integer;
begin
clrscr;
gotoxy(30,1); writeln('PROGRAM PERKALIAN');
gotoxy(30,2); writeln('-----------------');
writeln;
     for i:= 1 to 20 do
     begin
     textcolor(i);
          for j:=1 to 5 do
          begin
          hasil:=i*j;
          write(i:6 , 'x':2 , j:2 , '=':2 , hasil:3);
          end;
     writeln;
     end;
readln;
end.