program angka_terurut_dan_hasil;
uses crt;
var i, j, hasil      : integer;
begin
     clrscr;
     for i:=1 to 5 do
     begin
          for j:=1 to i do
          begin
          write(j,' ');
          hasil:=hasil+j;
          end;
     writeln('= ',hasil);
     hasil:=0;
     end;
     readln;
end.